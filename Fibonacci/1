#include <iostream>
using namespace std;

int main()
{
    int n;
    cout << "Enter the term you wanna find: " << endl;
    cin >> n;
    int a=0;
    int b = 1;
    int next=1;
    for (int i = 4; i <= n; i++)
    {
        a = b;
        b = next;
        next = a + b;
    }
    cout << next << endl;
    return 0;
}
