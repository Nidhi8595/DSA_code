#include<iostream>
using namespace std;
int main()
{
    int n,i=2;
    cout<<"Enter a number: "<<endl;
    cin>>n;
    while(i<n)
    {
        if(n%i!=0)
        {
            cout<<"Prime no. "<<endl;
        }
        else{
            cout<<"Not a prime no."<<endl;
        }
        i=i+1;
    }
    return 0;
}
