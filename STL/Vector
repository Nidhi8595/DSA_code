#include<bits/stdc++.h>
#include<vector>
using namespace std;

int main(){
    //one pointer from back
    vector<int>v1;
    v1.push_back(4);
    cout<<v1.capacity()<<endl;
    v1.push_back(9);
    cout<<v1.capacity()<<endl;
    v1.push_back(2);
    cout<<v1.capacity()<<endl;
    v1.push_back(6);
    v1.push_back(7);
    // v1.push_front(5); not allowed
    for(auto it:v1)
    {
        cout<<it<<" ";
    }
    cout<<endl;
    cout<<v1.capacity()<<endl;
    cout<<v1.size()<<endl;
    cout<<v1.at(1)<<endl;
    cout<<v1.back()<<endl;
    cout<<v1.front()<<endl;
    

    return 0;
}
