#include<iostream>
using namespace std;

int main(){
    int n=2;
    while(1)
    {
        switch (n)
        {
        case 1:
        {
            cout<<"1"<<endl;
            break;
        }
        case 2:
        {
            cout<<"2"<<endl;
            break;
        }
        default:
            break;
        }
        exit(3);
    }
    return 0;
}
